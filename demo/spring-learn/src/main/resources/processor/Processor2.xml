<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
	<bean id="studentService" name="studentService" class="com.lmw.learn.bean.processor.StudentService">
		<property name="name" value="LMW"></property>
		<property name="age" value="18"></property>
	</bean>
	<bean class="com.lmw.learn.bean.processor.BeanFactoryPostProcessor_1"></bean>
	<bean class="com.lmw.learn.bean.processor.BeanFactoryPostProcessor_2"></bean>
</beans>